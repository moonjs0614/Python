// 딕셔너리 연습
lux = {'health': 490, 'health': 800, 'mana': 331, 'melee': 550, 'armor': 18.72}
print(lux)

#키 에는 리스트와 딕셔너리 사용 X

lux1 = dict(health=490, mana=334, melee=550, armor=18.72)
print(lux1)

lux4 = dict({'health': 490, 'health': 800, 'mana': 331, 'melee': 550, 'armor': 18.72})
print(lux4)
print()

print(lux['health'], lux['armor'])
print()

lux['health'] = 2037
print(lux)
print('health' in lux)
print('attack_speed' in lux)
print(len(lux))
