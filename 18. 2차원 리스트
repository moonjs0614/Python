// 2차원 리스트 연습
a = [[10, 20],
     [30, 40],
     [50, 60]]
print(a[0][0])
for x, y in a:
    print(x, y)
print()
    
for i in a:
    for j in i:
        print(j, end=' ')
    print()
print()

for i in range(len(a)):
    for j in range(len(a[i])):
        print(a[i][j], end=' ')
print()
print()

i = 0
while i < len(a):
    x, y = a[i]
    print(x, y)
    i += 1
print()

while i < len(a):
    j = 0
    while j < len(a[j]):
        print(a[i][j], end=' ')
        j += 1
    print()
    i += 1
print()

a = []
for i in range(3):
    line = []
    for j in range(2):
        line.append(0)
    a.append(line)
print(a)
